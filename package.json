{
  "name": "todoapp",
  "version": "1.0.0",
  "description": "A todo app for manging tasks",
  "main": "server/app.js",
  "scripts": {
    "test:server": "NODE_ENV=test PORT=3333 nyc --reporter=html --reporter=text mocha --compilers js:babel-core/register ./server/tests/*.spec.js --timeout 10000",
    "test:client": "jest ./client/app/__tests__/*.spec.js --coverage",
    "test": "npm run test:server && npm run test:client",
    "build": "webpack -w",
    "dev": "babel-node server/app.js",
    "start": "webpack -p && node server/app.js ",
    "watch": "webpack -d --watch",
    "serve": "nodemon server/app.js"
  },
